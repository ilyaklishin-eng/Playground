{
  "name": "psyche-playground",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node server.mjs",
    "dev": "node server.mjs",
    "lint": "node tools/lint.mjs",
    "snapshot:check": "node tools/visual_snapshot.mjs",
    "snapshot:update": "node tools/visual_snapshot.mjs --update",
    "editorial:build": "node tools/editorial_pipeline.mjs",
    "smoke:http": "node tools/smoke_http_ci.mjs",
    "test": "node --test tests/*.test.mjs",
    "quality": "npm run lint && npm run test && npm run snapshot:check && npm run editorial:build"
  }
}
